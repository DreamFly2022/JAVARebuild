<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:myself="http://hefrankeleyn.hef.com/schema/myself/datasource"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://hefrankeleyn.hef.com/schema/myself/datasource
       http://hefrankeleyn.hef.com/schema/myself/datasource/datasource.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

    <myself:data-source id="xxx">
        <myself:dataSourceMap/>
        <myself:configurations>
            <bean class="com.hef.learn.springxbean.XRuleConfiguration">
                <property name="type" value="myself"/>
            </bean>
        </myself:configurations>
    </myself:data-source>



</beans>